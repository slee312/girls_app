<header class = "navbar navbar-fixed-top navbar-inverse">
  <div class = "navbar-inner">
    <div class = "container">
      <%= link_to "Pebbles", root_url, class: "logo" %>
      <span class = "pull-right">
        <ul class = "nav">
          <% if signed_in? %>
           <li><%= link_to "Home", home_url %></li> 
           <li><%= link_to "Explore", explore_url %></li> 
           <li><%= link_to "Help", help_url %></li> 
           <li><%= link_to "Sign out", signout_url %></li> 
          <% else %>
           <li><%= link_to "Sign in", "/auth/facebook" %></li> 
           <li><%= link_to "Help", help_url %></li> 
          <% end %>
        </ul>
      </span>
    </div>
  </div>
</header>
